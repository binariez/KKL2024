{
    "version": 2,
    "functions": {
        "api/*.php": { "runtime": "vercel-php@0.5.5" }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/api/index.php"
        },
        {
            "src": "/index.php",
            "headers": { "Location": "/api/index.php" },
            "dest": "/api/index.php"
        },
        {
            "src": "/client.php",
            "dest": "/api/client.php"
        },
        {
            "src": "/login.php",
            "dest": "/api/login.php"
        },
        {
            "src": "/logout.php",
            "dest": "/api/logout.php"
        },
        {
            "src": "/public/(.*)",
            "dest": "/$1"
        },
        {
            "src": "/public/*/(.*)",
            "dest": "/*/$1"
        }
    ]
}
